                android:textColor="@color/white"
                android:textAppearance="@style/custom_font"
                android:gravity="center"
                android:clickable="true"
                android:background="@drawable/keyboard_border"
                android:onClick="keyboardClicked"/>

            <TextView
                android:id="@+id/nTextView"
                android:tag="N"
                android:layout_width="30dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                android:layout_marginStart="5dp"
                android:text="N"
                android:textSize="20sp"
                android:textColor="@color/white"
                android:textAppearance="@style/custom_font"
                android:gravity="center"
                android:clickable="true"
                android:background="@drawable/keyboard_border"
                android:onClick="keyboardClicked"/>

            <TextView
                android:id="@+id/mTextView"
                android:tag="M"
                android:layout_width="30dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                android:layout_marginStart="5dp"
                android:text="M"
                android:textSize="20sp"
                android:textColor="@color/white"
                android:textAppearance="@style/custom_font"
                android:gravity="center"
                android:clickable="true"
                android:background="@drawable/keyboard_border"
                android:onClick="keyboardClicked"/>

            <TextView
                android:id="@+id/backspaceTextView"
                android:tag="backspace"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                android:layout_marginStart="5dp"
                android:textSize="20sp"
                android:textColor="@color/white"
                android:gravity="center"
                android:clickable="true"
                android:background="@drawable/keyboard_border"
                android:drawableStart="@drawable/backspace"
                android:onClick="keyboardClicked"/>

        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/resetImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/replay"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:onClick="resetGame"
        android:layout_margin="70dp"
        android:visibility="gone"  />

    <!--    Back Img-->
    <ImageView
        android:id="@+id/backimgview"
        android:contentDescription="@string/imageDescription"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="15dp"
        android:layout_marginStart="15dp"
        android:onClick="goToHome"
        android:background="@drawable/backicon"/>

    <!--    Home Img-->
    <ImageView
        android:id="@+id/homeimgview"
        android:contentDescription="@string/imageDescription"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="15dp"
        android:layout_centerHorizontal="true"
        android:onClick="goToHome"
        android:background="@drawable/homeicon"/>

    <!--    Setting Img-->
    <ImageView
        android:id="@+id/settingimgview"
        android:contentDescription="@string/imageDescription"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:layout_alignParentEnd="true"
        android:onClick="goToSetting"
        android:background="@drawable/settingsicon"/>

</RelativeLayout>
